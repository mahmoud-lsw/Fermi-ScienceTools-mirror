TLMIN# = 0.0   / minimum value
TLMAX# = 100.0 / maximum value
TTYPE# = 'CALENERGYSUM' / sum of the raw energies in all the crystals
TFORM# = 'E'   / data format of field: 4-byte REAL
TUNIT# = 'MeV' / physical unit of field
TLMIN# = 0.0   / minimum value
TLMAX# = 10000000. / maximum value
TTYPE# = 'CALTOTRLN' / total radiation lengths integrated along trajectory of first track
TFORM# = 'E'   / data format of field: 4-byte REAL
TUNIT# = ''    / physical unit of field: dimensionless
TLMIN# = 0.0   / minimum value
TLMAX# = 100.0 / maximum value
TTYPE# = 'IMGAMMAPROB' / classification tree probability that the event is a gamma-ray
TFORM# = 'E'   / data format of field: 4-byte REAL
TUNIT# = ''    / physical unit of field: dimensionless
TLMIN# = 0.0   / minimum value
TLMAX# = 1.0   / maximum value
TTYPE# = 'CONVERSION_POINT' /      reconstructed 3-space conversion point in instr. coords.
TFORM# = '3E' /  data format of field: 4-byte REAL
TUNIT# = 'm'  /  physical unit of field
TTYPE# = 'CONVERSION_LAYER' /      conversion layer in TKR, -1 means not in TKR
TFORM# = 'I'  /  data format of field: 2-byte INTEGER
TUNIT# = ''   /  physical unit of field: dimensionless
TLMIN# = -1   /  minimum value
TLMAX# = 18   /  maximum value

