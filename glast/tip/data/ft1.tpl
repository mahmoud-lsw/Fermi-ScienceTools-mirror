TFORM# = 'E'   / data format of field: 4-byte REAL
TUNIT# = ''    / physical unit of field: dimensionless
TLMIN# = 0.0   / minimum value
TLMAX# = 1.0   / maximum value
TTYPE# = 'IMCOREPROB' / classification tree probability that the event is in the core of the PSF
TFORM# = 'E'   / data format of field: 4-byte REAL
TUNIT# = ''    / physical unit of field: dimensionless
TLMIN# = 0.0   / minimum value
TLMAX# = 1.0   / maximum value
