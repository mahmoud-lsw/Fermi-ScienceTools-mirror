#  sed "s:--without-cxx::" | sed "s:--with-cxx=[^ 	]*::" |
#  sed "s:--without-f77::" | sed "s:--with-f77=[^ 	]*::" | \
#  sed "s:'*--with-lhea-subdirs=$with_lhea_subdirs'*::" | \
