if test "x$with_cxx" = x; then
  with_cxx=yes
fi
if test $with_cxx = no; then
  { echo "$as_me:$LINENO: Not looking for a C++ compiler.  C++ macros disabled." >&5
echo "$as_me: Not looking for a C++ compiler.  C++ macros disabled." >&6;}
  CXX=:

  GXX=no
else
  if test "x$CXX" = x; then
    if test "x${UNAME_SYSTEM}" = xSunOS; then
      for ac_prog in CC g++ c++
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
echo "$as_me:$LINENO: checking for $ac_word" >&5
echo $ECHO_N "checking for $ac_word... $ECHO_C" >&6
if test "${ac_cv_prog_CXX+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  if test -n "$CXX"; then
  ac_cv_prog_CXX="$CXX" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if $as_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_prog_CXX="$ac_prog"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done

fi
fi
