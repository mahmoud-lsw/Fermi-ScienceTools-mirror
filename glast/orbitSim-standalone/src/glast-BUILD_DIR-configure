  if test "x${UNAME_SYSTEM}" = "xIRIX" -a "x${F77_BASENAME}" = "xf90"; then
      { { echo "$as_me:$LINENO: error: IRIX Fortran 90 is not currently supported; please use f77" >&5
echo "$as_me: error: IRIX Fortran 90 is not currently supported; please use f77" >&2;}
   { (exit 1); exit 1; }; }
  fi
